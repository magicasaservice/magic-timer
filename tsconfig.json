{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "strictFunctionTypes": false,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true
  },
  "include": ["src/**/*"]
}
